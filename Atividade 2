import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner teclado = new Scanner(System.in);
    Double hp, lp, ha, la, area, dimensoes, necessario;
    System.out.println("digite a altura da parede");
    hp = teclado.nextDouble();
    System.out.println("digite a largura da parede");
    lp = teclado.nextDouble();
    System.out.println("area total da parede: ");
    area = hp * lp;
    System.out.println(area);
    System.out.println("digite a altura do azulejo");
    ha = teclado.nextDouble();
    System.out.println("digite a largura do azulejo");
    la = teclado.nextDouble();
    System.out.println("dimensoes do azulejo: ");
    dimensoes = ha * la;
    System.out.println(dimensoes);
    System.out.println("e necessario para revestir a parede: ");
    necessario = area/dimensoes;
    System.out.println(necessario)
