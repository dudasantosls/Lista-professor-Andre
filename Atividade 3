import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner enterScanner = new Scanner(System.in);
    
    int n1, n2, n3, n4, n5, teclado, soma;

     System.out.println("Digite a primeira nota");
    n1 = enterScanner.nextInt();
 System.out.println("Digite a segunda nota");
    n2 = enterScanner.nextInt();
 System.out.println("Digite a terceira nota");
    n3 = enterScanner.nextInt();
 System.out.println("Digite a quarta nota");
    n4 = enterScanner.nextInt();
 System.out.println("Digite a quinta nota");
    n5 = enterScanner.nextInt();
 
    soma = n1+n2+n3+n4+n5;
    System.out.println("a soma total das notas é:" + soma);
   
    if(soma >= 60) {
   System.out.println ("Aprovado");  
    } else if(soma < 40) {
  System.out.println ("Reprovado");
    } else if(soma <= 59) {
  System.out.println ("Em Recuperação");
      }
    }
}
